%% A very basic test script for launching of libexec subsystem through
%% perl with a 3 second timeout.

	set %pin "123"
	string %result
	libexec.3 libpnet.exe %pin results=&result
	echo "ERROR %script.error"
	echo "LIBEXEC RESULT %result"
	exit

